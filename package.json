{
  "name": "ce-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "unset DATABASE_URL DIRECT_URL && node scripts/select-db.js && next dev --turbopack",
    "dev:seed": "unset DATABASE_URL DIRECT_URL && node scripts/select-db.js && npm run db:seed && next dev --turbopack",
    "dev:debug": "unset DATABASE_URL DIRECT_URL && node scripts/select-db.js && DEBUG_DB=true next dev --turbopack",
    "build": "node scripts/setup-vercel-db.js && prisma generate && next build",
    "build:vercel": "./scripts/build-vercel.sh",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:seed": "prisma db seed",
    "db:test": "ts-node scripts/test-db-connection.ts",
    "sim:test-time": "ts-node scripts/verify-time-consistency.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
